program Indexacao {

global:

int A[10, 10];
int n;

functions:

int posic(int k, int l){
local:
int result;
statements:
result <- k + l;
return result;
}

main {

local:

    int i, j;

statements:
n <- 10;

for (i <- 0; i <= n-1; i <- i+1)
for (j <- 0; j <= n-1; j <- j+1){
A[i,j] <- posic(i, j);
}

for (i <- 0; i <= n-1; i <- i+1) {
for (j <- 0; j <= n-1; j <- j+1)
write (A[i,j], " ");
write ("\n");
}

}

}
