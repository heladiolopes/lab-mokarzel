program Primos {
global:
int i;

functions:

main {
local:
    	logic achou; int n, div, resto, cont, num;
statements:
	read (n);
	if (n > 0) {
		num <- 1; cont <- 0;
		while (cont < n) {
			num <- num + 1; div <- 2; achou <- false;
			while (achou = false && div * div <= num) {
				resto <- num % div;
				if (resto = 0) achou <- true;
				else div <- div + 1;
			}
			if (achou = false) {
				write (num, "\n"); cont <- cont + 1;
			}
		}
	}
}
}
